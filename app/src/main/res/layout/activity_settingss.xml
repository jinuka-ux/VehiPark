<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SettingsActivity">
    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="-12dp"
        app:srcCompat="@drawable/bottom" />
    <TextView
        android:id="@+id/textViewHelp"
        android:layout_width="103dp"
        android:layout_height="56dp"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="800px"
        android:drawableStart="@drawable/ic_help_vector"
        android:layout_marginBottom="0dp"
        android:fontFamily="@font/montserrat"
        android:gravity="center"
        android:text="Help"
        android:textColor="#ffffff"
        android:textSize="64px"
        android:textStyle="normal" />
    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="50dp"
        android:layout_height="55dp"
        android:layout_marginLeft="300px"
        app:srcCompat="@drawable/steer" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="174px"
        android:layout_height="140px"
        android:layout_marginTop="10px"
        app:srcCompat="@drawable/back" />
    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="420px"
        android:fontFamily="@font/poppins"
        android:layout_marginTop="20px"
        android:shadowColor="#000000"
        android:text="VehiPark"
        android:textColor="#000000"
        android:textSize="72px" />
    <TextView
        android:id="@+id/textDes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:layout_marginTop="180px"
        android:shadowColor="#000000"
        android:text="Settings"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_marginLeft="50px"
        android:textSize="80px" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="50px"
        android:layout_marginTop="320px"
        android:fontFamily="@font/montserrat"
        android:text="Name"
        android:textColor="#000000"
        android:textSize="48px" />

    <EditText
        android:id="@+id/regName"
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:layout_marginTop="390px"
        android:layout_marginLeft="50px"
        android:layout_marginRight="50px"
        android:background="@drawable/custom_input"
        android:drawablePadding="12dp"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ "
        android:text=""
        android:inputType="textCapWords"/>
    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="60px"
        android:layout_marginTop="495px"
        android:fontFamily="@font/montserrat"
        android:text="Address"
        android:textColor="#000000"
        android:textSize="48px" />
    <EditText
        android:id="@+id/regAddress"
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:layout_marginTop="575px"
        android:layout_marginLeft="50px"
        android:layout_marginRight="50px"
        android:background="@drawable/custom_input"
        android:drawablePadding="12dp"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:text="" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="60px"
        android:layout_marginTop="680px"
        android:fontFamily="@font/montserrat"
        android:text="Contact Number"
        android:textColor="#000000"
        android:textSize="48px" />
    <EditText
        android:id="@+id/regContact"
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:layout_marginTop="760px"
        android:layout_marginLeft="50px"
        android:layout_marginRight="50px"
        android:background="@drawable/custom_input"
        android:drawablePadding="12dp"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:digits="0123456789"
        android:maxLength="10"
        android:text="" />
    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="60px"
        android:layout_marginTop="880px"
        android:fontFamily="@font/montserrat"
        android:text="Email"
        android:textColor="#000000"
        android:textSize="48px" />
    <EditText
        android:id="@+id/regEmail"
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:layout_marginTop="960px"
        android:layout_marginRight="50px"
        android:layout_marginLeft="50px"
        android:background="@drawable/custom_input"
        android:drawablePadding="12dp"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ@.1234567890"
        android:text="" />
    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="60px"
        android:layout_marginTop="1200px"
        android:fontFamily="@font/montserrat"
        android:text="Change password?"
        android:textColor="#000000"
        android:textSize="48px" />
    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="540px"
        android:layout_marginTop="1200px"
        android:fontFamily="@font/montserrat"
        android:text="Click Here"
        android:textColor="#000000"
        android:textSize="48px" />
    <Button
        android:id="@+id/signUpButton"
        android:layout_width="151dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="599px"
        android:layout_marginTop="1420px"
        android:layout_centerHorizontal="true"
        android:text="Apply"
        app:cornerRadius="50px"
        app:rippleColor="#F23B5F" />

    <Button
        android:id="@+id/cancelButton"
        android:layout_width="151dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="599px"
        android:layout_marginTop="1580px"
        android:layout_centerHorizontal="true"
        android:text="Cancel"
        app:cornerRadius="50px"
        app:rippleColor="#F23B5F" />

</RelativeLayout>