<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BookingDetailsActivity">
    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="-12dp"
        app:srcCompat="@drawable/bottom" />
    <TextView
        android:id="@+id/textViewHelp"
        android:layout_width="103dp"
        android:layout_height="56dp"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="800px"
        android:drawableStart="@drawable/ic_help_vector"
        android:layout_marginBottom="0dp"
        android:fontFamily="@font/montserrat"
        android:gravity="center"
        android:text="Help"
        android:textColor="#ffffff"
        android:textSize="64px"
        android:textStyle="normal" />
    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="50dp"
        android:layout_height="55dp"
        android:layout_marginLeft="300px"
        app:srcCompat="@drawable/steer" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="174px"
        android:layout_height="140px"
        android:layout_marginTop="10px"
        app:srcCompat="@drawable/back" />
    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="420px"
        android:fontFamily="@font/poppins"
        android:layout_marginTop="20px"
        android:shadowColor="#000000"
        android:text="VehiPark"
        android:textColor="#000000"
        android:textSize="72px" />
    <TextView
        android:id="@+id/textDes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins"
        android:layout_marginTop="180px"
        android:shadowColor="#000000"
        android:text="Booking Details"
        android:textColor="#000000"
        android:layout_centerHorizontal="true"
        android:textSize="64px" />

    <Button
        android:id="@+id/confirmBookingDetails"
        android:layout_width="750px"
        android:layout_height="150px"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="1450px"
        android:text="Confirm Booking"
        app:cornerRadius="50px"
        app:rippleColor="#F23B5F" />
    <Button
        android:id="@+id/cancelBookingDetails"
        android:layout_width="750px"
        android:layout_height="150px"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="1600px"
        android:text="Cancel Booking"
        app:cornerRadius="50px"
        app:rippleColor="#F23B5F" />


    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="926px"
        android:layout_height="504px"
        android:layout_marginTop="350px"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/rectangledetails" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="814px"
        android:layout_height="390px"
        android:layout_marginTop="400px"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/subtractdetails" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="815px"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="500px"
        app:srcCompat="@drawable/line1" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ID 985564"
        android:fontFamily="@font/poppins"
        android:textColor="@color/black"
        android:textSize="64px"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="410px"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bihansa K."
        android:fontFamily="@font/poppins"
        android:textColor="@color/black"
        android:textSize="64px"
        android:textStyle="bold"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="530px"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Price Rs. 100"
        android:fontFamily="@font/poppins"
        android:textColor="@color/black"
        android:textSize="55px"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="610px"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pin xxxx"
        android:fontFamily="@font/poppins"
        android:textColor="@color/black"
        android:textSize="55px"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="685px"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Summary"
        android:fontFamily="@font/poppins"
        android:textColor="@color/black"
        android:textSize="64px"
        android:textStyle="bold"
        android:layout_marginLeft="50px"
        android:layout_marginTop="950px"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Park name"
        android:fontFamily="@font/montserrat"
        android:textColor="@color/black"
        android:textSize="55px"
        android:layout_marginLeft="50px"
        android:layout_marginTop="1070px"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Slot"
        android:fontFamily="@font/montserrat"
        android:textColor="@color/black"
        android:textSize="55px"
        android:layout_marginLeft="50px"
        android:layout_marginTop="1140px"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Duration"
        android:fontFamily="@font/montserrat"
        android:textColor="@color/black"
        android:textSize="55px"
        android:layout_marginLeft="50px"
        android:layout_marginTop="1210px"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Price per hour"
        android:fontFamily="@font/montserrat"
        android:textColor="@color/black"
        android:textSize="55px"
        android:layout_marginLeft="50px"
        android:layout_marginTop="1280px"/>


</RelativeLayout>