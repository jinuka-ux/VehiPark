        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="50dp"
            android:layout_height="55dp"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="300px"
            app:srcCompat="@drawable/steer" />
        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="420px"
            android:fontFamily="@font/poppins"
            android:layout_marginTop="23dp"
            android:shadowColor="#000000"
            android:text="VehiPark"
            android:textColor="#000000"
            android:textSize="72px" />
        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="98.06px"
            android:layout_height="97.94px"
            android:layout_marginLeft="900px"
            android:layout_marginRight="30px"
            android:layout_marginTop="20dp"
            android:layout_alignParentRight="true"
            app:srcCompat="@drawable/notification" />
        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="-12dp"
            app:srcCompat="@drawable/bottom" />
        <TextView
            android:id="@+id/textViewHelp"
            android:layout_width="103dp"
            android:layout_height="56dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="800px"
            android:drawableStart="@drawable/ic_help_vector"
            android:layout_marginBottom="0dp"
            android:fontFamily="@font/montserrat"
            android:gravity="center"
            android:text="Help"
            android:textColor="#ffffff"
            android:textSize="64px"
            android:textStyle="normal" />
        <TextView
            android:id="@+id/textDes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins"
            android:layout_marginTop="180px"
            android:shadowColor="#000000"
            android:layout_marginLeft="25px"
            android:text="Where do you\nwant to park?"
            android:textColor="#000000"
            android:textSize="72px" />
        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginTop="180px"
            app:srcCompat="@drawable/user" />
        <!--<EditText
            android:id="@+id/editSearchLocation"
            android:layout_width="match_parent"
            android:layout_height="120px"
            android:layout_marginTop="430px"
            android:layout_marginLeft="50px"
            android:layout_marginRight="50px"
            android:background="@drawable/custom_input2"
            android:drawableStart="@drawable/search1"
            android:drawablePadding="12dp"
