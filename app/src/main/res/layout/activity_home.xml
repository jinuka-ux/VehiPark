            android:fontFamily="@font/poppins"
            android:hint="Search location"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"
            android:textColorHint="@color/white" />-->
        <TextView
            android:layout_width="match_parent"
            android:layout_height="120px"
            android:id="@+id/text_view"
            android:layout_marginTop="430px"
            android:layout_marginLeft="50px"
            android:layout_marginRight="50px"
            android:textStyle="bold"
            android:hint="Select park"
            android:padding="12dp"
            android:gravity="center_vertical"
            android:drawableEnd="@drawable/ic_arrow"
            android:background="@android:drawable/editbox_background">
        </TextView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="170px"
            android:layout_marginTop="570px"
            android:fontFamily="@font/poppins"
            android:text="From"
            android:textColor="@color/black"
            android:textSize="60px"
            android:textStyle="normal"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="775px"
            android:layout_marginTop="570px"
            android:fontFamily="@font/poppins"
            android:text="To"
            android:textColor="@color/black"
            android:textSize="60px"
            android:textStyle="normal"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="660px"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tv_timer1"
                android:layout_width="400px"
                android:layout_height="wrap_content"
                android:background="@android:drawable/editbox_background"
                android:drawableTop="@drawable/ic_time"
                android:drawablePadding="10dp"
                android:gravity="center"
                android:layout_marginLeft="50px"
                android:textColorHighlight="@color/AppBlue"
                android:hint="Select time"
                android:textSize="24sp"
                android:textStyle="bold" />
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/ic_baseline_compare_arrows_24"
                android:layout_gravity="center"
                android:layout_marginHorizontal="20px"/>
            <TextView
                android:id="@+id/tv_timer2"
                android:layout_width="400px"
                android:layout_height="wrap_content"
                android:background="@android:drawable/editbox_background"
                android:drawableTop="@drawable/ic_time"
                android:drawablePadding="10dp"
                android:gravity="center"
                android:hint="Select time"
                android:textSize="24sp"
                android:textStyle="bold" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="900px"
            android:gravity="center">

            <RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <RadioButton
                    android:layout_width="288px"
                    android:layout_height="282.57px"
                    android:background="@drawable/radio_selector"
                    android:textColor="@drawable/radio_text_color"
                    android:drawableStart="@drawable/ic_car"
                    android:elevation="4dp"
                    android:padding="16dp"
                    android:layout_margin="16dp"/>
                <TextView
                    android:layout_width="300px"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/poppins"
                    android:text="Vehicle\nType"
                    android:textSize="64px"
                    android:textStyle="bold"
                    android:textAlignment="center"
                    android:textColor="@color/black">
                </TextView>
                <RadioButton
                    android:layout_width="288px"
                    android:layout_height="282.57px"
                    android:background="@drawable/radio_selector"
                    android:textColor="@drawable/radio_text_color"
                    android:drawableStart="@drawable/ic_bike"
                    android:elevation="4dp"
                    android:padding="16dp"
                    android:layout_margin="16dp"/>
            </RadioGroup>
        </LinearLayout>
        <Button
            android:id="@+id/buttonSendCode"
            android:layout_width="151dp"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="1250px"
            android:text="Continue"
            app:cornerRadius="50px"
            app:rippleColor="#F23B5F" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="980px"
            android:layout_height="400px"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="1400px"
            android:layout_marginBottom="70px"
            app:srcCompat="@drawable/homepic" />
    </RelativeLayout>

</androidx.drawerlayout.widget.DrawerLayout>