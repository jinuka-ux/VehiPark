<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vehipark">

    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VehiPark">
        <activity android:name=".example"></activity>
        <activity android:name=".BookingDispActivity" />
        <activity android:name=".FavouriteActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".FAQActivity" />
        <activity android:name=".BookingConfirmedActivity" />
        <activity android:name=".BookingDetailsActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".SlotSelectionActivity" />
        <activity android:name=".HomePageActivity" />
        <activity android:name=".PasswordUpdatedActivity" />
        <activity android:name=".NewCredentialsActivity" />
        <activity android:name=".CodeVerificationActivity" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LogInActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>